# Logger level
LOG_LEVEL=info

# The name of this application
APP_NAME='Darkdot'

# Substrate Node config
SUBSTRATE_URL=wss://rpc.darkdot.network

# Offchain config
OFFCHAIN_URL=https://app.darkdot.network/offchain

# IPFS config
# Port 5001 - IPFS Go with write access.
# Port 8080 - Read only.
IPFS_URL=https://app.darkdot.network/ipfs

# Notifications Web Socket
OFFCHAIN_WS=ws://app.darkdot.network:3011

# UI settings
UI_SHOW_ADVANCED=true
UI_SHOW_SEARCH=true
UI_SHOW_FEED=true
UI_SHOW_NOTIFICATIONS=true
UI_SHOW_ACTIVITY=true

# Integrate networks
KUSAMA_URL=wss://kusama-rpc.polkadot.io

# The id of the last storefront reserved at genesis. The first storefront has id 1.
LAST_RESERVED_STOREFRONT_ID=1000

# Ids of reserved storefronts that have been claimed.
CLAIMED_STOREFRONT_IDS=1,2,3,4,5

USE_OFFCHAIN_FOR_IPFS=true
DAG_HTTP_METHOD='POST'